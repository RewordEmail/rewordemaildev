{
  "version": 2,
  "builds": [
    {
      "src": "src/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server/server-full.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server-full.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
